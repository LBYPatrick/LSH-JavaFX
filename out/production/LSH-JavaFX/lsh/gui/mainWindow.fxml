<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ProgressBar?>
<GridPane fx:controller="lsh.gui.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <ChoiceBox fx:id="solution_box" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <Button fx:id="execute" GridPane.rowIndex="1" GridPane.columnIndex="1" textAlignment="CENTER" text="FIX" onMouseClicked="#runSolution"/>
    <ProgressBar fx:id="pBar" GridPane.rowIndex="2"/>
</GridPane>